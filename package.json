{
  "name": "@zhuzy/react-shared",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "unpkg": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "clean": "rimraf dist && rimraf es && rimraf lib",
    "build": "pnpm clean && rollup --config rollup.config.ts --configPlugin typescript",
    "test": "jest --maxWorkers 2",
    "test:ssr": "jest --maxWorkers 2 --config ./jest.config.node.ts",
    "test:coverage": "jest --coverage",
    "debug": "node --inspect-brk src/index.js",
    "lint": "eslint --ext .ts,.js ./src",
    "ci": "pnpm install && pnpm run lint && pnpm run test",
    "version:git": "git add . && git commit -m \"chore(release): publish ${npm_package_version}\"",
    "version:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version:firstchangelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "publish": "pnpm publish",
    "publish:beta": "pnpm publish --tag beta"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ],
    "*.{css,less,scss}": [
      "stylelint --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-external-helpers": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "^29.5.1",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.1.5",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-localstorage-mock": "^2.4.26",
    "lint-staged": "^13.2.2",
    "picocolors": "^1.0.0",
    "pnpm": "^8.5.1",
    "postcss": "^8.4.23",
    "postcss-nesting": "^11.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-two-style-order": "^1.0.1",
    "raf-stub": "^3.0.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.7",
    "rollup-plugin-license": "^3.0.1",
    "rollup-plugin-postcss": "^4.0.2",
    "stylelint": "^15.6.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.5",
    "@babel/runtime-corejs3": "^7.21.5",
    "@zhuzy/utils": "^0.1.3",
    "copy-to-clipboard": "^3.3.3",
    "core-js": "^3.30.2",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "screenfull": "^6.0.2"
  },
  "peerDependencies": {
    "@types/node": ">= 14",
    "react": "^16.8.0  || ^17.0.0 || ^18.0.0",
    "react-dom": ">= 16.9.0"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  }
}
